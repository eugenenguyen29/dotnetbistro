{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "dotnetbistro",
  "compatibility_date": "2025-01-17",
  "assets": {
    "directory": "./public",
    "html_handling": "auto-trailing-slash",
    "not_found_handling": "404-page"
  },
  "build": {
    "command": "hugo --minify"
  },
  "routes": [
    {
      "pattern": "blog.dotnetbistro.cloud",
      "custom_domain": true
    }
  ]
}
